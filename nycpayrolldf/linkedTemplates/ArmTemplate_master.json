{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "nycpayrolldf"
		},
		"linkgen2storage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'linkgen2storage'"
		},
		"linksqldb_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'linksqldb'"
		},
		"linkgen2storage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsnycpayroll.dfs.core.windows.net/"
		},
		"linksqldb_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "dsnycpayroll.database.windows.net"
		},
		"linksqldb_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "db_nycpayroll"
		},
		"linksqldb_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqladmin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "nycpayrolldf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"linkgen2storage_accountKey": {
						"value": "[parameters('linkgen2storage_accountKey')]"
					},
					"linksqldb_password": {
						"value": "[parameters('linksqldb_password')]"
					},
					"linkgen2storage_properties_typeProperties_url": {
						"value": "[parameters('linkgen2storage_properties_typeProperties_url')]"
					},
					"linksqldb_properties_typeProperties_server": {
						"value": "[parameters('linksqldb_properties_typeProperties_server')]"
					},
					"linksqldb_properties_typeProperties_database": {
						"value": "[parameters('linksqldb_properties_typeProperties_database')]"
					},
					"linksqldb_properties_typeProperties_userName": {
						"value": "[parameters('linksqldb_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}